[core]
    excludesFile = ~/.config/git/ignore
[alias]
	st = status -sb
	co = checkout
	lol = log --graph --all --oneline --full-history --date-order --date=short --color --abbrev-commit --decorate
	ci = commit
	l1 = log --oneline
	a = add
	b = branch --sort=-committerdate
	bm = branch --merged
	bnm = branch --no-merged
	ci = commit
	co = checkout
	h = help
	hc = help config
	lf = log --pretty=fuller
	s = status
[log]
	abbrevCommit = true
	date = local
	decorate = short
